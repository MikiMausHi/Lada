{
  "name": "my-education",
  "version": "1.0",
  "description": "Best project",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "start": "better-npm-run start",
    "start:test": "better-npm-run start:test",
    "start:prod": "better-npm-run start:prod",
    "build": "yarn clean && better-npm-run build",
    "clean": "better-npm-run clean",
    "analyze": "yarn clean && better-npm-run analyze"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start:test": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "testing",
        "PORT": 8090
      }
    },
    "start:prod": {
      "command": "node .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8090
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf ./public/*"
    },
    "analyze": {
      "command": "webpack -p --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "analyze"
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@loadable/babel-plugin",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console",
          "transform-react-remove-prop-types"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "react"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack/config.babel.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-trailing-spaces": "off",
      "padded-blocks": "off",
      "object-curly-newline": "off",
      "no-prototype-builtins": "off",
      "global-require": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "max-len": [
        "error",
        {
          "code": 500
        }
      ],
      "react/forbid-prop-types": "off",
      "react/require-default-props": "off",
      "react/destructuring-assignment": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-cycle": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "to"
          ]
        }
      ]
    },
    "globals": {
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DEV__": true,
      "__TEST__": false
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js",
      "src/core/utils/renderHtml.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@farbenmeer/react-spring-slider": "^1.1.1",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/react": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "@u-wave/react-youtube": "^0.7.0",
    "accept-language": "^3.0.18",
    "ajv": "^6.11.0",
    "arui-feather": "^17.0.0",
